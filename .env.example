# Coinbase MiniKit Configuration
NEXT_PUBLIC_MINIKIT_API_KEY=your_minikit_api_key_here
NEXT_PUBLIC_ONCHAINKIT_API_KEY=your_onchainkit_api_key_here

# DEX Aggregator API Keys
NEXT_PUBLIC_1INCH_API_KEY=your_1inch_api_key_here
NEXT_PUBLIC_PARASWAP_API_KEY=your_paraswap_api_key_here

# Cross-chain Bridge API Keys
NEXT_PUBLIC_ACROSS_API_KEY=your_across_api_key_here
NEXT_PUBLIC_CELER_API_KEY=your_celer_api_key_here

# Gas Relay/Paymaster Configuration
NEXT_PUBLIC_PAYMASTER_URL=https://your-paymaster-url.com
PAYMASTER_PRIVATE_KEY=your_paymaster_private_key_here

# Database Configuration
DATABASE_TYPE=memory
DATABASE_URL=postgresql://username:password@localhost:5432/aggroswap

# External API Keys
NEXT_PUBLIC_ETHERSCAN_API_KEY=your_etherscan_api_key_here
NEXT_PUBLIC_INFURA_PROJECT_ID=your_infura_project_id_here
NEXT_PUBLIC_ALCHEMY_API_KEY=your_alchemy_api_key_here

# Security
JWT_SECRET=your_jwt_secret_here
API_SECRET_KEY=your_api_secret_key_here

# Monitoring and Logging
SENTRY_DSN=your_sentry_dsn_here
LOG_LEVEL=info

# Feature Flags
ENABLE_GAS_RELAY=true
ENABLE_CROSS_CHAIN=true
ENABLE_API_ACCESS=true

# Rate Limiting
API_RATE_LIMIT_BASIC=60
API_RATE_LIMIT_PREMIUM=300

# Transaction Settings
DEFAULT_SLIPPAGE=0.5
MAX_SLIPPAGE=5.0
SWAP_FEE_PERCENTAGE=0.1
MINIMUM_SWAP_FEE=0.5

# Network Configuration
SUPPORTED_CHAIN_IDS=8453,1,137,42161
DEFAULT_CHAIN_ID=8453

# Cache Settings
QUOTE_CACHE_DURATION=30000
TRANSACTION_TIMEOUT=300000

# Development Settings
NODE_ENV=production
NEXT_PUBLIC_APP_URL=https://your-app-domain.com

